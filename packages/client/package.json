{
  "name": "@lens-social/client",
  "version": "0.0.0",
  "description": "Low-level Lens API client",
  "repository": {
    "directory": "packages/client",
    "type": "git",
    "url": "git://github.com/lens-network/social-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "import": ["./dist/index.esm.d.ts"],
      "require": ["./dist/index.cjs.d.ts"]
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "dependencies": {
    "@lens-social/graphql": "workspace:*",
    "@lens-social/types": "workspace:*",
    "@urql/core": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^22.7.8",
    "jest-extended": "^4.0.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "license": "MIT"
}

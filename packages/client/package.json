{
  "name": "@lens-social/client",
  "version": "0.0.0",
  "description": "Low-level Lens API client",
  "repository": {
    "directory": "packages/client",
    "type": "git",
    "url": "git://github.com/lens-network/social-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "import": ["./dist/index.d.ts"],
      "require": ["./dist/index.d.cts"]
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@lens-social/env": "workspace:*",
    "@lens-social/graphql": "workspace:*",
    "@lens-social/types": "workspace:*",
    "@urql/core": "^5.0.8",
    "loglevel": "^1.9.2"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.3",
    "viem": "^2.21.33"
  },
  "license": "MIT"
}

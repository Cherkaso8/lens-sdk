{
  "name": "@lens-social/graphql",
  "version": "0.0.0",
  "description": "GraphQL bindings for the Lens API",
  "repository": {
    "directory": "packages/types",
    "type": "git",
    "url": "git://github.com/lens-social/sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "typesVersions": {
    "*": {
      "import": [
        "./dist/index.esm.d.ts"
      ],
      "require": [
        "./dist/index.cjs.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "doctor": "gql.tada doctor",
    "download:local": "gql-tada generate schema 'http://localhost:3000/graphql' --output './schema.graphql'",
    "download:staging": "gql-tada generate schema 'https://api.staging.lens.dev/graphql' --output './schema.graphql'",
    "generate": "gql-tada generate output"
  },
  "dependencies": {
    "@lens-social/types": "workspace:*",
    "gql.tada": "^1.8.10",
    "graphql": "^16.9.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "license": "MIT"
}
